{
  "name": "pbx",
  "version": "1.0.0-alpha-16",
  "description": "Modular server implementation based on Patchboard API schema",
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "coffee --nodejs --harmony --compile -o ./lib ./src",
    "watch": "coffee --nodejs --harmony --compile --watch -o lib/ ./src",
    "test": "coffee --nodejs --harmony test/index.coffee",
    "tag": "(node_modules/.bin/json -f package.json version | xargs -I version git tag -am version version) && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/pandastrike/pbx.git"
  },
  "author": "Dan Yoder <dan@pandastrike.com>",
  "license": "MIT",
  "devDependencies": {
    "amen": "^1.0.0-alpha-03",
    "js-yaml": "^3.2.5",
    "json": "^9.0.3",
    "pirate": "^1.0.0-alpha-05"
  },
  "dependencies": {
    "fairmont": "^1.0.0-alpha-17",
    "jsck": "^0.2.1",
    "routington": "^1.0.2",
    "shred": "^1.1.0-alpha-04",
    "when": "^3.6.3"
  },
  "engines": {
    "node": ">=0.11"
  }
}
